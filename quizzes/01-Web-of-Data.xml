<quiz>

<question type="truefalse">
  <name>
    <text>Persistent Identifiers</text>
  </name>
     <questiontext format="html">
       <text>Is 26978244 a globally unique identifier for the PubMed article on the FAIR principles?</text>
     </questiontext>

   <answer fraction="100">
      <text>Yes</text>
      <feedback><text>Sorry. 26978244 is unique within PubMed but is not globally unique.  </text></feedback>
   </answer>
   <answer fraction="0">
      <text>No</text>
      <feedback><text>Correct!</text></feedback>
   </answer>
</question>

<question type="shortanswer">
